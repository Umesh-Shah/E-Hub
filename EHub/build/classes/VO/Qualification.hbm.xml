<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="VO.Qualification" table="Qualifications">
   <id name="quali_id" type="long" column="ID" >
   <generator class="increment"/>
  </id>

  <property name="qualificationName">
   <column name="QualificationName"/>
  </property>
  <property name="qualificationDes">
  <column name="QualificationDescription"/>
  </property>
  
 </class>
</hibernate-mapping>